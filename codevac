vacs1<- read.csv(file.choose(),header=TRUE)#Overall United States cases
lm1=lm(case~total_vaccinations+total_distributed+people_vaccinated+people_fully_vaccinated_per_hundred+total_vaccinations_per_hundred+people_fully_vaccinated+ people_vaccinated_per_hundred+distributed_per_hundred+daily_vaccinations_raw+daily_vaccinations+daily_vaccinations_per_million+share_doses_used,data=vacs1)
shapiro.test(vacs1$total_vaccinations)
shapiro.test(vacs1$total_distributed)
shapiro.test(vacs1$people_vaccinated)
shapiro.test(vacs1$people_fully_vaccinated_per_hundred)
shapiro.test(vacs1$total_vaccinations_per_hundred)
shapiro.test(vacs1$people_fully_vaccinated)
shapiro.test(vacs1$people_vaccinated_per_hundred)
shapiro.test(vacs1$distributed_per_hundred)
shapiro.test(vacs1$daily_vaccinations_raw)
shapiro.test(vacs1$daily_vaccinations)
shapiro.test(vacs1$daily_vaccinations_per_million)
shapiro.test(vacs1$share_doses_used)

hist(vacs1$total_vaccinations)
hist(vacs1$total_distributed)
hist(vacs1$people_vaccinated)
hist(vacs1$people_fully_vaccinated_per_hundred)
hist(vacs1$total_vaccinations_per_hundred)
hist(vacs1$people_fully_vaccinated)
hist(vacs1$people_vaccinated_per_hundred)
hist(vacs1$distributed_per_hundred)
hist(vacs1$daily_vaccinations_raw)
hist(vacs1$daily_vaccinations+daily_vaccinations_per_million)
hist(vacs1$daily_vaccinations_per_million)
hist(vacs1$share_doses_used)

qqnorm(vacs1$total_vaccinations, pch = 1, frame = FALSE)
qqline(vacs1$total_vaccinations, col = "steelblue", lwd = 2)
qqnorm(vacs1$total_distributed, pch = 1, frame = FALSE)
qqline(vacs1$total_distributed, col = "steelblue", lwd = 2)
qqnorm(vacs1$people_vaccinated, pch = 1, frame = FALSE)
qqline(vacs1$people_vaccinated, col = "steelblue", lwd = 2)
qqnorm(vacs1$people_fully_vaccinated_per_hundred, pch = 1, frame = FALSE)
qqline(vacs1$people_fully_vaccinated_per_hundred, col = "steelblue", lwd = 2)
qqnorm(vacs1$total_vaccinations_per_hundred, pch = 1, frame = FALSE)
qqline(vacs1$total_vaccinations_per_hundred, col = "steelblue", lwd = 2)
qqnorm(vacs1$people_fully_vaccinated, pch = 1, frame = FALSE)
qqline(vacs1$people_fully_vaccinated, col = "steelblue", lwd = 2)
qqnorm(vacs1$people_vaccinated_per_hundred, pch = 1, frame = FALSE)
qqline(vacs1$people_vaccinated_per_hundred, col = "steelblue", lwd = 2)
qqnorm(vacs1$distributed_per_hundred, pch = 1, frame = FALSE)
qqline(vacs1$distributed_per_hundred, col = "steelblue", lwd = 2)
qqnorm(vacs1$daily_vaccinations_raw, pch = 1, frame = FALSE)
qqline(vacs1$daily_vaccinations_raw, col = "steelblue", lwd = 2)
qqnorm(vacs1$daily_vaccinations, pch = 1, frame = FALSE)
qqline(vacs1$daily_vaccinations_raw, col = "steelblue", lwd = 2)
qqnorm(vacs1$share_doses_use, pch = 1, frame = FALSE)
qqline(vacs1$share_doses_use, col = "steelblue", lwd = 2)





cor.test(vacs1$total_vaccinations,vacs1$case)
cor.test(vacs1$total_distributed,vacs1$case)
cor.test(vacs1$people_vaccinated,vacs1$case)
cor.test(vacs1$people_fully_vaccinated_per_hundred,vacs1$case)
cor.test(vacs1$total_vaccinations_per_hundred,vacs1$case)
cor.test(vacs1$people_fully_vaccinated,vacs1$case)
cor.test(vacs1$people_vaccinated_per_hundred,vacs1$case)
cor.test(vacs1$distributed_per_hundred,vacs1$case)
cor.test(vacs1$daily_vaccinations_raw,vacs1$case)
cor.test(vacs1$daily_vaccinations,vacs1$case)
cor.test(vacs1$daily_vaccinations_per_million,vacs1$case)
cor.test(vacs1$share_doses_used,vacs1$case)
